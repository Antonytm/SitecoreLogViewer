{
  "manifest_version": 2,

  "name": "SitecoreShowLog",
  "description": "This extension shows latest log Sitecore's log file",
  "version": "1.0",

  "permissions": [
	"tabs", "storage"
  ],
  "plugins": [{"path": "npapi-file-io-32.dll", "public": true}],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}